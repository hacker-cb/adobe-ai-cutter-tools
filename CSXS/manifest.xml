<?xml version='1.0' encoding='UTF-8'?>

<ExtensionManifest ExtensionBundleId="com.hackercb.cuttertools" ExtensionBundleVersion="1.0.0" Version="7.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ExtensionList>
    <Extension Id="com.hackercb.cuttertools" Version="1.0.0" />
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="ILST" Version="22"/>
    </HostList>
    <LocaleList>
      <Locale Code="All" />
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="8.0" />
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.hackercb.cuttertools">
      <DispatchInfo>
        <Resources>
          <MainPath>./index.html</MainPath>
          <!--
            We use JSX.
            https://creative-scripts.com/jsx-js/#jsx
            https://community.adobe.com/t5/illustrator-discussions/reload-cep-panel-extension-and-its-extendscript-without-restarting-illustrator/td-p/10844579
          -->
          <!-- <ScriptPath>./jsx/index.jsx</ScriptPath> -->
          <CEFCommandLine />
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>Cutter Tools</Menu>
          <Geometry>
            <Size>
              <Height>400</Height>
              <Width>350</Width>
            </Size>
          </Geometry>
          <Icons />
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>